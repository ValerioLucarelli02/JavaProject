package main;

import java.util.ArrayList;

public class Utente {

	String ID, Password;
	boolean Admin;

	public Utente(String ID, String Password) {
		super();
		this.ID = ID;
		this.Password = Password;
	}

	public String getID() {
		return ID;
	}

	public void setID(String iD) {
		ID = iD;
	}

	public String getPassword() {
		return Password;
	}

	public void setPassword(String password) {
		Password = password;
	}

}
